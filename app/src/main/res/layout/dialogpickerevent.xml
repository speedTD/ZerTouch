                                android:textColor="#000000"
                                android:textSize="@dimen/_11sdp"
                                android:layout_width="match_parent"
                                android:text="@string/panel_main_control"
                                android:layout_height="0dp"/>
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:weightSum="10"
                        android:layout_weight="2"
                        android:layout_width="match_parent"
                        android:layout_height="0dp">
                    <LinearLayout
                            android:layout_weight="5"
                            android:weightSum="10"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                        <ImageView
                                android:focusable="true"
                                android:clickable="true"
                                android:id="@+id/event3_img"
                                android:padding="@dimen/_8sdp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/ic_layoutresents"
                                android:layout_weight="7"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"/>
                        <TextView
                                android:id="@+id/event3"
                                android:layout_weight="3"
                                android:gravity="center"
                                android:textColor="#000000"
                                android:textSize="@dimen/_11sdp"
                                android:layout_width="match_parent"
                                android:text="@string/panel_main_Recents"
                                android:layout_height="0dp"/>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_weight="5"
                            android:weightSum="10"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                        <ImageView
                                android:id="@+id/event5_img"
                                android:padding="@dimen/_8sdp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/ic_layoutnotification"
                                android:layout_weight="7"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"/>
                        <TextView
                                android:id="@+id/event5"
                                android:layout_weight="3"
                                android:gravity="center"
                                android:textColor="#000000"
                                android:textSize="@dimen/_11sdp"
                                android:layout_width="match_parent"
                                android:text="@string/panel_main_notification"
                                android:layout_height="0dp"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:weightSum="10"
                        android:layout_weight="2"
                        android:layout_width="match_parent"
                        android:layout_height="0dp">
                    <LinearLayout
                            android:layout_weight="5"
                            android:weightSum="10"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                        <ImageView
                                android:id="@+id/event4_img"
                                android:padding="@dimen/_8sdp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/ic_layouthome"
                                android:layout_weight="7"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"/>

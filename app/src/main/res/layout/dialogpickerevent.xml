                                android:gravity="center"
                                android:textColor="#000000"
                                android:textSize="@dimen/_11sdp"
                                android:layout_width="match_parent"
                                android:text="@string/panel_main_lockscreen"
                                android:layout_height="0dp"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:weightSum="10"
                        android:layout_weight="2"
                        android:layout_width="match_parent"
                        android:layout_height="0dp">
                    <LinearLayout
                            android:layout_weight="5"
                            android:weightSum="10"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                        <ImageView
                                android:id="@+id/event9_img"
                                android:scaleType="fitCenter"
                                android:padding="@dimen/_8sdp"
                                android:src="@drawable/ic_layoutvolumedown"
                                android:layout_weight="7"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"/>
                        <TextView
                                android:id="@+id/event9"
                                android:layout_weight="3"
                                android:gravity="center"
                                android:textColor="#020202"
                                android:textSize="@dimen/_11sdp"
                                android:layout_width="match_parent"
                                android:text="@string/panel_main_lowsound"
                                android:layout_height="0dp"/>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_weight="5"
                            android:weightSum="10"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                        <ImageView
                                android:id="@+id/event10_img"
                                android:padding="@dimen/_8sdp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/ic_layoutvolumeup"
                                android:layout_weight="7"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"/>
                        <TextView
                                android:id="@+id/event10"
                                android:layout_weight="3"
                                android:gravity="center"
                                android:textColor="#000000"
                                android:textSize="@dimen/_11sdp"
                                android:layout_width="match_parent"
                                android:text="@string/panel_main_upsound"
                                android:layout_height="0dp"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_weight="2"
                        android:layout_width="match_parent"
                        android:layout_height="0dp">
                    <LinearLayout
                            android:layout_weight="5"
                            android:weightSum="10"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                        <ImageView
                                android:id="@+id/event11_img"
                                android:scaleType="fitCenter"
                                android:padding="@dimen/_8sdp"
                                android:src="@drawable/ic_screenshotic"
                                android:layout_weight="7"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"/>
                        <TextView
                                android:id="@+id/event11"
                                android:layout_weight="3"
                                android:gravity="center"
                                android:textColor="#020202"
                                android:textSize="@dimen/_11sdp"
                                android:layout_width="match_parent"
                                android:text="@string/Screnshot"
                                android:layout_height="0dp"/>
                    </LinearLayout>

                    <LinearLayout
                            android:layout_weight="5"
                            android:weightSum="10"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                        <ImageView

                                android:scaleType="fitCenter"
                                android:padding="@dimen/_8sdp"

                                android:layout_weight="7"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"/>
                        <TextView

                                android:layout_weight="3"
                                android:gravity="center"
                                android:textColor="#020202"
                                android:textSize="@dimen/_11sdp"
                                android:layout_width="match_parent"

                                android:layout_height="0dp"/>
                    </LinearLayout>


                </LinearLayout>
            </LinearLayout>

        </RelativeLayout>


</RelativeLayout>
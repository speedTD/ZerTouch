<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" android:layout_gravity="center"
        android:id="@+id/layout_layouttable"
        android:background="@drawable/customdialog"
        android:gravity="center"
        android:layout_width="@dimen/_250sdp"
        android:layout_height="wrap_content">
<!--    <ScrollView android:layout_width="match_parent"-->
<!--                android:layout_height="match_parent">-->
        <RelativeLayout
                android:layout_margin="@dimen/_10sdp"
                       android:layout_width="match_parent"
                        android:layout_height="wrap_content">


            <LinearLayout
                    android:id="@+id/vl"
                    android:weightSum="13"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_400sdp"
                    android:orientation="vertical">
                <TextView
                        android:layout_weight="1"
                        android:id="@+id/title_dialogevent"
                        android:textColor="#f000"
                        android:textSize="@dimen/_20sdp"
                        android:gravity="center"
                        android:text="@string/title_dialog_event"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"/>

                <LinearLayout
                        android:weightSum="10"
                        android:layout_weight="2"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:orientation="horizontal">
                    <LinearLayout
                            android:layout_weight="5"
                            android:weightSum="10"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            tools:ignore="NestedWeights">
                        <ImageView
                                android:focusable="true"
                                android:clickable="true"
                                android:id="@+id/event1_img"
                                android:padding="@dimen/_8sdp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/ic_layoutapp"
                                android:layout_weight="7"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"/>
                        <TextView
                                android:id="@+id/event1"
                                android:layout_weight="3"
                                android:gravity="center"
                                android:textColor="#000000"
                                android:textSize="@dimen/_11sdp"
                                android:layout_width="match_parent"
                                android:text="@string/panel_main_app"
                                android:layout_height="0dp"/>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_weight="5"
                            android:weightSum="10"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                        <ImageView
                                android:id="@+id/event6_img"
                                android:padding="@dimen/_8sdp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/ic_layoutringmode"
                                android:layout_weight="7"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"/>
                        <TextView
                                android:id="@+id/event6"
                                android:layout_weight="3"
                                android:gravity="center"
                                android:textColor="#090909"
                                android:textSize="@dimen/_11sdp"
                                android:layout_width="match_parent"
                                android:text="@string/panel_main_ringmode"
                                android:layout_height="0dp"/>
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                        android:weightSum="10"
                        android:layout_weight="2"
                        android:layout_width="match_parent"
                        android:layout_height="0dp">
                    <LinearLayout
                            android:layout_weight="5"
                            android:weightSum="10"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                        <ImageView
                                android:focusable="true"
                                android:clickable="true"
                                android:id="@+id/event2_img"
                                android:padding="@dimen/_8sdp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/ic_layoutback"
                                android:layout_weight="7"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"/>
                        <TextView
                                android:id="@+id/event2"
                                android:layout_weight="3"
                                android:gravity="center"
                                android:textColor="#000000"
                                android:textSize="@dimen/_11sdp"
                                android:layout_width="match_parent"
                                android:text="@string/panel_main_back"
                                android:layout_height="0dp"/>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_weight="5"
                            android:weightSum="10"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                        <ImageView
                                android:id="@+id/event7_img"
                                android:padding="@dimen/_8sdp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/ic_layoutcontrol"
                                android:layout_weight="7"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"/>

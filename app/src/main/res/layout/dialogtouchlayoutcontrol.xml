<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_gravity="center|fill"
        android:layout_width="match_parent"
        android:layout_height="fill_parent">
    <RelativeLayout

            android:background="#00000000"
            android:visibility="visible"
            android:gravity="center"
            android:id="@+id/dialog_fullTouchLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <LinearLayout
                android:weightSum="3"
                android:id="@+id/dialog_layouttable"
                android:background="@drawable/customlayouttouch"
                android:gravity="center"
                android:layout_width="@dimen/_250sdp"
                android:layout_height="@dimen/_250sdp"
                android:orientation="vertical">
            <LinearLayout
                    android:layout_marginTop="@dimen/_15sdp"
                    android:layout_marginStart="@dimen/_30sdp"
                    android:layout_marginEnd="@dimen/_30sdp"
                    android:layout_marginBottom="@dimen/_10sdp"
                    android:weightSum="10"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                     android:orientation="horizontal">
                <LinearLayout
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:weightSum="10"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <ImageView
                            android:id="@+id/control_tuxoay_img"
                            android:scaleType="fitCenter"
                            android:padding="@dimen/_10sdp"
                            android:src="@drawable/ic_panelrotate_off"
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"/>
                <TextView
                        android:layout_weight="2"
                        android:id="@+id/control_tuxoay"
                        android:gravity="center"
                        android:textColor="@color/colorOff"
                        android:textSize="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:text="@string/panel_secondary_auto_rotate"
                        android:layout_height="0dp"/>
                </LinearLayout>
                <LinearLayout
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:weightSum="10"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <ImageView
                            android:id="@+id/control_maybay_img"
                            android:scaleType="fitCenter"
                            android:padding="@dimen/_10sdp"
                            android:src="@drawable/ic_panelplane_off"
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"/>
                <TextView
                        android:id="@+id/control_maybay"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:textColor="@color/colorOff"
                        android:textSize="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:text="@string/panel_secondary_plane"
                        android:layout_height="0dp"/>
                </LinearLayout>

            </LinearLayout>
            <LinearLayout

                    android:weightSum="9"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp">
                <LinearLayout
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:weightSum="10"
                        android:layout_weight="3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <ImageView
                            android:id="@+id/control_bluetooth_img"
                            android:scaleType="fitCenter"
                            android:padding="@dimen/_10sdp"
                            android:src="@drawable/ic_panelbluetooth_off"
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"/>
                <TextView
                        android:id="@+id/control_bluetooth"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:textColor="@color/colorOff"
                        android:textSize="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:text="@string/panel_secondary_bluetooth"
                        android:layout_height="0dp"/>
                </LinearLayout>
                <ImageView
                        android:padding="@dimen/_17sdp"
                        android:background="#00FFFFFF"
                        android:layout_weight="3"
                        android:gravity="center"
                        android:id="@+id/control_backmain"
                        android:src="@drawable/ic_backmain"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"/>
                <LinearLayout
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:weightSum="10"
                        android:layout_weight="3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <ImageView
                            android:id="@+id/control_chedosang_img"
                            android:scaleType="fitCenter"
                            android:padding="@dimen/_10sdp"
                            android:src="@drawable/ic_panellight_off"
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"/>
                <TextView
                        android:id="@+id/control_chedosang"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:textColor="@color/colorOff"
                        android:textSize="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:text="@string/panel_secondary_light"
                        android:layout_height="0dp"/>
                </LinearLayout>

            </LinearLayout>
            <LinearLayout
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginBottom="@dimen/_15sdp"
                    android:layout_marginLeft="@dimen/_25sdp"
                    android:layout_marginRight="@dimen/_25sdp"
                    android:weightSum="10"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp">
                <LinearLayout
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:weightSum="10"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <ImageView
                            android:id="@+id/control_wifi_img"
                            android:scaleType="fitCenter"
                            android:padding="@dimen/_10sdp"
                            android:src="@drawable/ic_panelwifi_off"
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"/>

                <TextView
                        android:id="@+id/control_wifi"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:textColor="@color/colorOff"
                        android:textSize="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:text="@string/panel_secondary_wifi"
                        android:layout_height="0dp"/>
                </LinearLayout>
                <LinearLayout
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:weightSum="10"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <ImageView
                            android:id="@+id/control_denpin_img"
                            android:scaleType="fitCenter"
                            android:padding="@dimen/_10sdp"
                            android:src="@drawable/ic_panelflashlight_off"
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"/>
                <TextView
                        android:id="@+id/control_denpin"
                        android:layout_weight="2"
                        android:gravity="center"
                        android:textColor="@color/colorOff"
                        android:textSize="@dimen/_10sdp"
                        android:layout_width="match_parent"
                        android:text="@string/panel_secondary_Flash"
                        android:layout_height="0dp"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </RelativeLayout>
</FrameLayout>
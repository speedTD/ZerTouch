<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_gravity="center|fill"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <RelativeLayout
            android:id="@+id/layout_ads"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50sdp"
            android:layout_alignParentBottom="true"
            android:visibility="gone"/>
    <RelativeLayout
            android:layout_above="@id/layout_ads"
            android:background="#00000000"
            android:visibility="visible"
            android:gravity="center"
            android:id="@+id/maintouch_fullTouchLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <TextView
                android:layout_marginTop="@dimen/_20sdp"
                android:id="@+id/vailo"
                android:gravity="center"
                android:textColor="#000000"
                android:textSize="@dimen/_13sdp"
                android:layout_width="match_parent"
                android:text="@string/layout_preview"
                android:layout_height="wrap_content"/>
        <LinearLayout
                android:layout_centerInParent="true"
                android:layout_below="@id/vailo"
                android:weightSum="3"
                android:id="@+id/layout_layouttable"
                android:background="@drawable/customlayouttouch"
                android:gravity="center"
                android:layout_width="@dimen/_250sdp"
                android:layout_height="@dimen/_250sdp"
                android:orientation="vertical" android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginBottom="@dimen/_20sdp"
                android:layout_marginLeft="@dimen/_30sdp"
                android:layout_marginStart="@dimen/_20sdp"
                android:layout_marginRight="@dimen/_20sdp">
            <LinearLayout
                    android:layout_marginTop="@dimen/_10sdp"
                    android:weightSum="9"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:orientation="horizontal">
                <LinearLayout
                        android:weightSum="10"
                        android:layout_weight="3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <FrameLayout

                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp">

                        <ImageView
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:background="@drawable/point"
                                android:layout_marginStart="@dimen/_20sdp"
                                android:layout_marginEnd="@dimen/_20sdp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        <ImageView
                                android:id="@+id/layout_ungdung_img"
                                android:scaleType="fitCenter"
                                android:padding="@dimen/_14sdp"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:src="@drawable/ic_panelapp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                    </FrameLayout>
                    <TextView
                            android:layout_weight="2"
                            android:id="@+id/layout_ungdung"
                            android:gravity="center"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="match_parent"
                            android:text="@string/panel_main_app"
                            android:layout_height="0dp"/>
                </LinearLayout>
                <LinearLayout
                        android:weightSum="10"
                        android:layout_weight="3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <FrameLayout
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp">

                        <ImageView
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:background="@drawable/point"
                                android:layout_marginStart="@dimen/_20sdp"
                                android:layout_marginEnd="@dimen/_20sdp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        <ImageView
                                android:id="@+id/layout_khoamanhinh_img"
                                android:scaleType="fitCenter"
                                android:padding="@dimen/_14sdp"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:src="@drawable/ic_panellock"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                    </FrameLayout>

                    <TextView
                            android:layout_weight="2"
                            android:id="@+id/layout_khoamanhinh"
                            android:gravity="center"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="match_parent"
                            android:text="@string/panel_main_lockscreen"
                            android:layout_height="0dp"/>
                </LinearLayout>
                <LinearLayout
                        android:weightSum="10"
                        android:layout_weight="3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <FrameLayout
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp">

                        <ImageView
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:background="@drawable/point"
                                android:layout_marginStart="@dimen/_20sdp"
                                android:layout_marginEnd="@dimen/_20sdp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        <ImageView
                                android:id="@+id/layout_dieukhien_img"
                                android:scaleType="fitCenter"
                                android:padding="@dimen/_14sdp"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:src="@drawable/ic_panelcontrol"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                    </FrameLayout>

                    <TextView
                            android:id="@+id/layout_dieukhien"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="match_parent"
                            android:text="@string/panel_main_control"
                            android:layout_height="0dp"/>
                </LinearLayout>

            </LinearLayout>
            <LinearLayout
                    android:layout_marginTop="@dimen/_10sdp"
                    android:weightSum="9"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp">
                <LinearLayout
                        android:weightSum="10"
                        android:layout_weight="3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <FrameLayout
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp">

                        <ImageView
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:background="@drawable/point"
                                android:layout_marginStart="@dimen/_20sdp"
                                android:layout_marginEnd="@dimen/_20sdp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        <ImageView
                                android:id="@+id/layout_quaylai_img"
                                android:scaleType="fitCenter"
                                android:padding="@dimen/_14sdp"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:src="@drawable/ic_panelbacksytem"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                    </FrameLayout>

                    <TextView
                            android:layout_weight="2"
                            android:id="@+id/layout_quaylai"
                            android:gravity="center"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/_12sdp"
                            android:layout_width="match_parent"
                            android:text="@string/panel_main_back"
                            android:layout_height="0dp"/>
                </LinearLayout>
                <TextView

                        android:layout_weight="3"
                        android:gravity="center"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"/>
                <LinearLayout
                        android:weightSum="10"
                        android:layout_weight="3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <FrameLayout
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp">

                        <ImageView
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:background="@drawable/point"
                                android:layout_marginStart="@dimen/_20sdp"
                                android:layout_marginEnd="@dimen/_20sdp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        <ImageView
                                android:id="@+id/layout_chedoam_img"
                                android:scaleType="fitCenter"
                                android:padding="@dimen/_14sdp"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:src="@drawable/ic_panelringmode"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                    </FrameLayout>

                    <TextView
                            android:layout_weight="2"
                            android:id="@+id/layout_chedoam"
                            android:gravity="center"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="match_parent"
                            android:text="@string/panel_main_ringmode"
                            android:layout_height="0dp"/>
                </LinearLayout>

            </LinearLayout>
            <LinearLayout
                    android:layout_marginBottom="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:weightSum="9"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp" android:orientation="horizontal">
                <LinearLayout
                        android:weightSum="10"
                        android:layout_weight="3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <FrameLayout
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp">

                        <ImageView
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:background="@drawable/point"
                                android:layout_marginStart="@dimen/_20sdp"
                                android:layout_marginEnd="@dimen/_20sdp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        <ImageView
                                android:id="@+id/layout_danhiem_img"
                                android:scaleType="fitCenter"
                                android:padding="@dimen/_14sdp"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:src="@drawable/ic_panelrecentss"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                    </FrameLayout>

                    <TextView
                            android:layout_weight="2"
                            android:id="@+id/layout_danhiem"
                            android:gravity="center"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="match_parent"
                            android:text="@string/panel_main_Recents"
                            android:layout_height="0dp"/>
                </LinearLayout>
                <LinearLayout
                        android:weightSum="10"
                        android:layout_weight="3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <FrameLayout
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp">

                        <ImageView
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:background="@drawable/point"
                                android:layout_marginStart="@dimen/_20sdp"
                                android:layout_marginEnd="@dimen/_20sdp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        <ImageView
                                android:id="@+id/layout_manhinhchinh_img"
                                android:scaleType="fitCenter"
                                android:padding="@dimen/_14sdp"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:src="@drawable/ic_panelhome"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                    </FrameLayout>

                    <TextView
                            android:layout_weight="2"
                            android:id="@+id/layout_manhinhchinh"
                            android:gravity="center"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="match_parent"
                            android:text="@string/panel_main_home"
                            android:layout_height="0dp"/>
                </LinearLayout>
                <LinearLayout
                        android:weightSum="10"
                        android:layout_weight="3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                    <FrameLayout
                            android:layout_weight="8"
                            android:layout_width="match_parent"
                            android:layout_height="0dp">

                        <ImageView
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:background="@drawable/point"
                                android:layout_marginStart="@dimen/_20sdp"
                                android:layout_marginEnd="@dimen/_20sdp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        <ImageView
                                android:id="@+id/layout_thongbao_img"
                                android:scaleType="fitCenter"
                                android:padding="@dimen/_14sdp"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:src="@drawable/ic_panelnotification"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                    </FrameLayout>

                    <TextView
                            android:id="@+id/layout_thongbao"
                            android:layout_weight="2"
                            android:gravity="center"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="match_parent"
                            android:text="@string/panel_main_notification"
                            android:layout_height="0dp"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <TextView
                android:drawablePadding="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_60sdp"
                android:layout_marginEnd="@dimen/_60sdp"
                android:layout_below="@id/layout_layouttable"
                android:layout_marginTop="@dimen/_20sdp"
                android:gravity="center"
                android:textColor="#000000"
                android:textSize="@dimen/_13sdp"
                android:layout_width="match_parent"
                android:text="@string/layout_text_main"
                android:layout_height="wrap_content"/>

    </RelativeLayout>


</RelativeLayout>